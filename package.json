{
  "name": "ydb",
  "version": "1.3.4",
  "description": "A local database that supports yaml and json",
  "url": "https://onrirr.github.io/yDB/#/",
  "repository": "github:onrirr/ydb",
  "main": "./dist/esm/index.js",
  "categories": [
    "database",
    "yaml",
    "yml",
    "json",
    "quick.db",
    "quickdb",
    "ydb.js",
    "ydb"
  ],
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build:clean": "rm -rf ./dist",
    "build": "npm-run-all build:clean compile && node ./scripts/prepare-package-json",
    "start": "npx nodemon"
  },
  "exports": {
  ".": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "types": "./dist/types/index.d.ts",
    "default": "./dist/esm/index.js"
  },
  "./dist/esm/*": "./dist/esm/*.js",
  "./dist/cjs/*": "./dist/cjs/*.js",
  "./dist/types/*": "./dist/types/*.d.ts"
  },
  "author": "onrir",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.3.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "string-crypto": "^2.0.2",
    "yaml": "^2.3.1"
  }
}
